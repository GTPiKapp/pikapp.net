{
  "name": "pikapp.net",
  "version": "1.0.0",
  "description": "PiKapp.net website",
  "main": "./bin/www",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "prestart": "npm run build",
    "start": "node ./bin/www",
    "migrate": "knex migrate:latest",
    "frontend": "webpack -d --watch",
    "backend": "nodemon ./bin/www --ignore public/ --ignore frontend/",
    "dev": "knex migrate:latest && concurrently -k \"npm run frontend\" \"npm run backend\""
  },
